'use strict';

const { addCommand } = require('./Commands')

/**
 * @param {string} prefix
 * @param {string} name
 * @param {string} reply
 * @param {boolean} ping
 */

async function createCommand({ prefix, name, reply, ping, permissions }) {
    addCommand({ prefix, name, reply, ping, permissions })
}

module.exports = createCommand;
